{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ng2-select2",
  "version": "1.0.0-beta.16",
  "description": "Angular2 wrapper for select2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "ng-packagr -p package.json"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "author": "Nejc Zdovc <nejc.zdovc@3zsistemi.si>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NejcZdovc/ng2-select2/issues"
  },
  "homepage": "https://github.com/NejcZdovc/ng2-select2",
  "repository": {
    "type": "git",
    "url": "https://github.com/NejcZdovc/ng2-select2.git"
  },
  "keywords": [
    "angular2",
    "select2",
    "wrap"
  ],
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "@angular/common": ">=11.0.0",
    "@angular/core": ">=11.0.0",
    "@angular/platform-browser": ">=11.0.0",
    "rxjs": "^6.4.0",
    "zone.js": ">=0.9.1"
  },
  "devDependencies": {
    "@angular/common": "11.2.14",
    "@angular/compiler": "11.2.14",
    "@angular/compiler-cli": "11.2.14",
    "@angular/core": "11.2.14",
    "@angular/platform-browser": "11.2.14",
    "@types/select2": "4.0.42",
    "@types/jquery": "^2.0.39",
    "ng-packagr": "5.7.1",
    "rimraf": "^3.0.0",
    "rxjs": "^6.4.0",
    "tslint": "^5.20.0",
    "typescript": "4.1.5",
    "zone.js": "~0.9.1"
  }
}
